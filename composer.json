{
    "name": "jackardios/elastic-query-wizard",
    "description": "Laravel Elastic Query Wizard",
    "keywords": [
        "jackardios",
        "scout-query-wizard",
        "laravel-elastic-query-wizard",
        "elastic-query-builder",
        "laravel-query-builder",
        "laravel",
        "scout",
        "elastic",
        "query",
        "wizard",
        "builder",
        "json",
        "api",
        "json api",
        "query builder"
    ],
    "homepage": "https://github.com/jackardios/elastic-query-wizard",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jackardios/elastic-query-wizard/issues",
        "source": "https://github.com/jackardios/elastic-query-wizard"
    },
    "authors": [
        {
            "name": "Salavat Salakhutdinov",
            "email": "salavat.salahutdinov@gmail.com",
            "homepage": "https://github.com/jackardios"
        }
    ],
    "require": {
        "php": "^8.1",
        "jackardios/elastic-scout-driver-plus": "^v4.0.0",
        "jackardios/laravel-query-wizard": "^v2.0.2",
        "laravel/framework": "^v10.0"
    },
    "require-dev": {
        "ext-json": "*",
        "babenkoivan/elastic-migrations": "^v3.4.0",
        "laravel/legacy-factories": "^1.0.4",
        "laravel/scout": "^v10.8.0",
        "jackardios/laravel-eloquent-spatial": "^2.0",
        "orchestra/testbench": "^v8.21.1",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Jackardios\\ElasticQueryWizard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jackardios\\ElasticQueryWizard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
